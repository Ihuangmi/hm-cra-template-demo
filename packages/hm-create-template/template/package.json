{
  "name": "nr-trade-haihui-mobile-html",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production vite build --mode production",
    "build:test": "cross-env NODE_ENV=test vite build --mode test",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --cache --fix \"**/*.{less,postcss,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prettier": "prettier -c --write \"src/**/*\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "ahooks": "^3.3.10",
    "antd-mobile": "^5.12.1",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.19.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "nprogress": "^0.2.0",
    "qs": "^6.10.3",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.3-alpha.2",
    "unstated-next": "^1.1.0",
    "xgplayer": "^2.31.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.32",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.0",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.0",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@windicss/plugin-animations": "^1.0.9",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-define-config": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.4.1",
    "path": "^0.12.7",
    "postcss": "^8.4.13",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.8.2",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svgr": "^2.1.0",
    "vite-plugin-windicss": "^1.8.4"
  }
